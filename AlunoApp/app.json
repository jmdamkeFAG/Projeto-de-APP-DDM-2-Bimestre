{
  "expo": {
    "name": "ClassControl",
    "slug": "ClassControl",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ClassControlLogo.png",
    "scheme": "classcontrol",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jmdamke.ClassControl",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ClassControlLogo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.jmdamke.ClassControl"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/ClassControlLogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/ClassControlLogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9692a061-f405-43d8-b2f7-5f9b98a353a1"
      }
    }
  }
}
